--finding number of staff by gender
select count (gender), gender
from employees
group by gender


--how many APBET schools does PACE work with
select*
from schools
where school_name like '%APBET%'


--which schools does Corazon directly work with?
select school_id, school_name
from schools
where employee_id = 008
